{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace",
      "HubConnection": "Warning"
    }
  },
  "oidc": {
    "Authority": "https://localhost:5000/",
    "ClientId": "vmsclient",
    "DefaultScopes": [
      "openid",
      "profile",
      "email",
      "vms.client"
    ],
    "PostLogoutRedirectUri": "https://localhost:5002/",
    "RedirectUri": "https://localhost:5002/authentication/login-callback",
    "ResponseType": "code"
  }
}
